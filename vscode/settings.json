{
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "workbench.iconTheme": "eq-material-theme-icons-ocean",
  "breadcrumbs.enabled": false,
  "explorer.compactFolders": false,
  "files.trimTrailingWhitespace": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": false,
    "source.organizeImports": false,
  },
  "tabnine.experimentalAutoImports": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "editor.fontFamily": "JetBrainsMono Nerd Font",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.cursorStyle": "line",
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.statusBar.enabled": false,
  "gitlens.defaultDateLocale": null,
  "vsicons.dontShowNewVersionMessage": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "explorer.openEditors.visible": 0,
  "workbench.editor.tabCloseButton": "off",
  "workbench.statusBar.visible": false,
  "tailwindCSS.includeLanguages": {
    "plaintext": "javascript"
  },
  "editor.fontLigatures": false,
  "json.schemas": [],
  "editor.lineHeight": 1.5,
  "editor.fontSize": 15,
  "gitlens.views.repositories.files.layout": "list",
  "gitlens.views.repositories.showWorktrees": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "[python]": {
    "editor.formatOnType": true
  },
  "window.zoomLevel": -1,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    },
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "s"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "f", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "f", "s"],
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["<leader>", "/"],
      "commands": ["actions.find"]
    },
    {
      "before": ["<leader>", "s"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "k"],
      "commands": [":tabn"]
    },
    {
      "before": ["<leader>", "j"],
      "commands": [":tabp"]
    },
    {
      "before": ["<leader>", "n"],
      "commands": [":tabnew"]
    },
    {
      "before": ["1"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
     {
      "before": ["<leader>", "q"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["workbench.action.terminal.toggleTerminal"]
    },
  ],
  "window.titleBarStyle": "custom",
  "vim.leader": "<space>",
  "editor.lineNumbers": "relative",
  "workbench.colorTheme": "Bearded Theme Arc EolStorm",
  "workbench.colorCustomizations": {
    "editor.background": "#161617",
    "minimap.background": "#161617",
    "activityBar.background": "#1f1f1f",
    "editorGutter.background": "#161617",
    "editorGroupHeader.tabsBackground": "#161617",
    "tab.activeBackground": "#161617",
    "tab.inactiveBackground": "#161617",
    "sideBar.background": "#161617",
    "titleBar.activeBackground": "#161617",
    "titleBar.inactiveBackground": "#161617",
    "terminal.background": "#161617",
    "menu.background": "#1f1f1f",
    "menu.foreground": "#c9c9c9",
    "panel.background": "#1f1f1f"
  },
  "editor.minimap.enabled": false,
  "editor.tokenColorCustomizations": {
    "[Bearded Theme Arc EolStorm]": {
      "strings": "#6A9F60",
      "textMateRules": [
        {
          "scope": "string.quoted.single.ts",
          "settings": {
            "foreground": "#6A9F60"
          }
        }
      ]
    }
  },
  "editor.semanticTokenColorCustomizations": {
    "[Bearded Theme Arc EolStorm]": {
      "rules": {
        "parameter": "#679891",
        "string": "#6A9F60"
      }
    }
  },
  "explorer.confirmDelete": false
}
